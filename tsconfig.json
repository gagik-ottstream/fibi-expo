{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "paths": {
      "$/*": [ "./*" ],
      "$screens/*": ["./src/screens/*"],
      "$components/*": ["./src/components/*"],
      "$layouts/*": ["./src/layouts/*"],
      "$providers/*": ["./src/providers/*"],
      "$resources/*": ["./src/resources/*"],
      "$services/*": ["./src/services/*"],
      "$store/*": ["./src/store/*"],
      "$uikit/*": ["./src/uikit/*"],
      "$models/*": ["./src/models/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "./declarations.d.ts",
        ".eslintrc.js",
        "./src",
         ".eslintrc.js",
         "./configs"
  ],
  
}
